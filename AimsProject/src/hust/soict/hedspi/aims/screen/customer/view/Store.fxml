<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/11.0.14-internal"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="hust.soict.hedspi.aims.screen.customer.controller.ViewStoreController ">
   <padding>
      <Insets left="20.0"/>
   </padding>
   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
      <Label prefHeight="200.0" prefWidth="200.0" text="AIMS" textFill="#004cff">
         <font>
            <Font size="50.0"/>
         </font>
         <padding>
            <Insets left="10.0"/>
         </padding>
      </Label>
      <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS"/>
      <Button minHeight="50.0" mnemonicParsing="false" onAction="#btnViewCartPressed" prefWidth="100.0"
              text="View Cart">
         <HBox.margin>
            <Insets right="20.0"/>
         </HBox.margin>
      </Button>
   </HBox>
   <ScrollPane prefHeight="760.0" prefWidth="1024.0">
      <GridPane fx:id="gridPane" prefHeight="0.0" prefWidth="0.0">
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
         </rowConstraints>
      </GridPane>
   </ScrollPane>
</VBox>
